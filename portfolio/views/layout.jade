    //-  Contact
    section#contact
      .container
        .row
          .col-lg-12.text-center
            h2.section-heading.text-uppercase Contact Us
            h3.section-subheading.text-muted Lorem ipsum dolor sit amet consectetur.

        .row
          .col-lg-12
            form#contactForm(name="sentMessage" novalidate="novalidate")
              .row
                .col-md-6
                  .form-group
                    input.form-control#name(type="text" placeholder="Your Name *" required="required" data-validation-required-message="Please enter your name.")
                    p.help-block.text-danger
                  .form-group
                    input.form-control#email(type="text" placeholder="Your Email *" required="required" data-validation-required-message="Please enter your email address.")
                    p.help-block.text-danger
                  .form-group
                    input.form-control#phone(type="text" placeholder="Your Phone *" required="required" data-validation-required-message="Please enter your phone number.")
                    p.help-block.text-danger
                .col-md-6    
                  .form-group
                    textarea.form-control#message(type="text" placeholder="Your Message *" required="required" data-validation-required-message="Please enter a message.")
                    p.help-block.text-danger  
                .clearfix
                  .col-lg-12.text-center  
                    .success  
                    button#sendMessageButton.btn.btn-primary.btn-xl.text-uppercase(type="submit") Send message

